{
  "auto_append_newline": true,
  "auto_detect_indentation": true,
  "default_encoding": "UTF-8",
  "default_project_location": "~/Documents/lectures",
  "editor_keybindings": "vim",
  "font_size_points": 11,
  "highlight_r_function_calls": true,
  "initial_working_directory": "~/Documents/dev",
  "jobs_tab_visibility": "shown",
  "load_workspace": false,
  "new_proj_git_init": true,
  "new_proj_use_renv": true,
  "posix_terminal_shell": "custom",
  "rainbow_parentheses": true,
  "remove_history_duplicates": true,
  "restore_last_project": false,
  "restore_source_documents": false,
  "save_workspace": "never",
  "scroll_past_end_of_document": true,
  "source_with_echo": true,
  "strip_trailing_whitespace": true,
  "tab_multiline_completion": true,
  "cran_mirror": {
    "name": "Global (CDN)",
    "host": "RStudio",
    "url": "https://cran.rstudio.com/",
    "country": "us",
    "ok": 1,
    "secondary": ""
  },
  "rmd_chunk_output_inline": false,
  "custom_shell_command": "/opt/homebrew/bin/zsh",
  "panes": {
    "quadrants": [
      "Source",
      "TabSet1",
      "Console",
      "TabSet2"
    ],
    "tabSet1": [
      "Environment",
      "History",
      "Connections",
      "Build",
      "VCS",
      "Tutorial",
      "Presentation"
    ],
    "tabSet2": [
      "Files",
      "Plots",
      "Packages",
      "Help",
      "Viewer",
      "Presentations"
    ],
    "hiddenTabSet": [],
    "console_left_on_top": false,
    "console_right_on_top": true,
    "additional_source_columns": 0
  }
}
